{
    "name": "snowcap/admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SnowcapAdminBundle",
    "keywords": ["admin"],
    "homepage": "https://github.com/snowcap/SnowcapAdminBundle",
    "license": "MIT",
    "authors": [
